{
  "name": "openmapsamples-maplibre",
  "version": "0.1.0",
  "description": "Provides an OpenMapSamples control in MapLibre",
  "type": "module",
  "scripts": {
    "start": "rm -Rf .parcel-cache dist && parcel index.html --open",
    "build": "rm -rf .parcel-cache dist && parcel build index.html --public-url ./"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adamfranco/OpenMapSamples-MapLibre.git"
  },
  "keywords": [
    "osm",
    "OpenStreetMap",
    "geojson",
    "maplibre",
    "map"
  ],
  "author": "Adam Franco",
  "license": "LGPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/adamfranco/OpenMapSamples-MapLibre/issues"
  },
  "homepage": "https://github.com/adamfranco/OpenMapSamples-MapLibre#readme",
  "dependencies": {
    "maplibre-gl": "^2.0",
    "openmapsamples": "github:adamfranco/OpenMapSamples"
  },
  "devDependencies": {
    "dotenv": "^16.0.0",
    "maplibre-gl-inspect": "github:acalcutt/maplibre-gl-inspect",
    "parcel": "^2.2.1",
    "parcel-plugin-nuke-dist": "^1.0.1"
  }
}
